{
    "name": "maxakawizard/po-parser",
    "description": "Gettext *.po parser for PHP",
    "keywords": ["po", "gettext", "i18n", "l10n"],
    "homepage": "http://github.com/MAXakaWIZARD/PoParser",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Max Grigorian",
            "email": "maxakawizard@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "~3.0",
        "php-coveralls/php-coveralls": "~2.5",
        "phpunit/phpunit": ">7 <10"
    },
    "autoload": {
        "psr-4": {"PoParser\\": "src/"}
    },
    "scripts": {
        "phpcs": "phpcs -p --standard=PSR12 --extensions=php src",
        "test": [
            "@phpcs",
            "phpunit"
        ]
    }
}
